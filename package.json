{
  "name": "slide-anim",
  "version": "1.3.0",
  "author": "Yomotsu",
  "license": "MIT",
  "repository": "yomotsu/slide-anim",
  "jsnext:main": "dist/slide-anim.module.js",
  "module": "dist/slide-anim.module.js",
  "main": "dist/slide-anim.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "rollup": "^0.65.0",
    "rollup-plugin-typescript2": "^0.17.0",
    "rollup-watch": "^4.3.1",
    "typescript": "^3.0.3",
